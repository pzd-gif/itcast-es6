<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pzd.mapper.CaseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.pzd.entity.Case">
        <id column="id" property="id" />
        <result column="case_num" property="caseNum" />
        <result column="case_desc" property="caseDesc" />
        <result column="expect" property="expect" />
        <result column="function_moudel" property="functionMoudel" />
        <result column="all_num" property="allNum" />
        <result column="success_num" property="successNum" />
        <result column="fail_num" property="failNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, case_num, case_desc, expect, function_moudel, all_num, success_num, fail_num
    </sql>

</mapper>
